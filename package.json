{
  "name": "electrontemplate",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "version": "0.0.2",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --config.asar=false",
    "unpacked": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@iconify-json/akar-icons": "^1.1.1",
    "@iconify-json/ant-design": "^1.1.0",
    "@iconify-json/bi": "^1.1.1",
    "@iconify-json/bx": "^1.1.0",
    "@iconify-json/bytesize": "^1.1.0",
    "@iconify-json/carbon": "^1.1.1",
    "@iconify-json/clarity": "^1.1.1",
    "@iconify-json/codicon": "^1.1.3",
    "@iconify-json/emojione": "^1.1.1",
    "@iconify-json/entypo": "^1.1.0",
    "@iconify-json/gridicons": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@vitejs/plugin-vue": "2.2.2",
    "@vue/test-utils": "2.0.0-rc.18",
    "cross-env": "7.0.3",
    "electron": "22.0.0",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.29.0",
    "eslint-plugin-vue": "9.8.0",
    "happy-dom": "8.1.0",
    "nano-staged": "0.8.0",
    "playwright": "1.28.1",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.4",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-icons": "^0.13.2",
    "unplugin-vue-components": "^0.17.21",
    "vite": "4.0.0",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-windicss": "^1.8.1",
    "vitest": "0.5.4",
    "vue-tsc": "1.0.12"
    "windicss": "^3.5.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.5",
    "@heroicons/vue": "^1.1.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@vueuse/core": "^7.7.1",
    "@vueuse/head": "^0.7.5",
    "axios": "^1.3.4",
    "create-desktop-shortcuts": "^1.10.1",
    "electron-log": "^4.4.8",
    "electron-settings": "^4.0.2",
    "electron-updater": "^5.3.0",
    "node-fetch": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.56.2",
    "sweetalert2": "^11.6.15",
    "typedi": "^0.10.0",
    "unzipper": "^0.10.11",
    "vue": "3.2.45",
    "vue-router": "^4.1.6"
  }
}
